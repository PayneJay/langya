<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="jack.demo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".activity.MoreTextViewActivity"></activity>
        <activity android:name=".activity.CollapsedActivity"></activity>
        <activity android:name=".activity.ProgressViewActivity"></activity>
        <activity android:name=".activity.FlexboxLayoutActivity"></activity>
        <activity android:name=".activity.FlowLayoutActivity"></activity>
        <activity android:name=".activity.ScrollTableViewActicity"></activity>
        <activity android:name=".activity.CalendarActivity"></activity>
    </application>

</manifest>